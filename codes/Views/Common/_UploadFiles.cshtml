


@{
    ViewBag.Title = "_UploadFile";
    Layout = null;
}
<h2>File Upload</h2>

@using (Html.BeginForm("UploadFile", "Common", null,FormMethod.Post,new { enctype = "multipart/form-data" }))
{
    <label for="file">Upload File:</label>
    <input type="file" name="file" id="file" /><br><br>
    <input type="submit" value="Upload File"  />
    <br><br>
    @ViewBag.Message
}
