@model List<Compliance.DataObject.Menus>
<div class="toggle">
    <span class="icon-bar"></span>
    <span class="icon-bar"></span>
    <span class="icon-bar"></span>
</div>
<div class="left_panel">
    <div class="top_panel">
        <h3>
            @if (Session["username"] != null)
            {
                @Session["username"].ToString()<br />
                <span>@Session["emailid"].ToString()</span>
            }
        </h3>
        <p><a href="/UserManagement/Logout">Logout</a></p>
    </div>
    <ul>
        @foreach (var menu in Model)
        {
            <li><a href="@menu.PathUrl?pid=@menu.Id" data-toggle="tooltip" title="@menu.MenuName">@menu.MenuName <span class="iconnew"><img style="width:30px;height:30px" src="~/Content/assets/images/@menu.icon" alt="" />@*<i class="fa fa-users fa-2x"></i></span>*@</a></li>
        }
    </ul>

